    <!-- Three -->
    <section id="three" class="wrapper">
      <div class="inner">
        <header class="align-center">
          <h2>General contact</h2>
          <p>If you do not know who to contact, use this form </p>
        </header>
        <form method="post" action="#">
          <div class="row uniform">
            <div class="6u 12u$(xsmall)"> <input name="name" id="name" value=""
                placeholder="Name" type="text"> </div>
            <div class="6u$ 12u$(xsmall)"> <input name="email" id="email" value=""
                placeholder="Email" type="email"> </div>
            <!-- Break -->
            <div class="12u$">
              <div class="select-wrapper">
                <select name="category" id="category">
                  <option value="">- Category -</option>
                  <option value="1">Manufacturing</option>
                  <option value="1">Shipping</option>
                  <option value="1">Administration</option>
                  <option value="1">Human Resources</option>
                </select>
              </div>
            </div>
            <!-- Break -->
            <div class="4u 12u$(small)"> <input id="priority-low" name="priority"
                checked="checked" type="radio"> <label for="priority-low">Low
                Priority</label> </div>
            <div class="4u 12u$(small)"> <input id="priority-normal" name="priority"
                type="radio"> <label for="priority-normal">Normal Priority</label>
            </div>
            <div class="4u$ 12u$(small)"> <input id="priority-high" name="priority"
                type="radio"> <label for="priority-high">High Priority</label>
            </div>
            <!-- Break -->
            <div class="6u 12u$(small)"> <input id="copy" name="copy" type="checkbox">
              <label for="copy">Email me a copy of this message</label> </div>
            <div class="6u$ 12u$(small)"> <input id="human" name="human" checked="checked"
                type="checkbox"> <label for="human">I am a human and not a
                robot</label> </div>
            <!-- Break -->
            <div class="12u$"> <textarea name="message" id="message" placeholder="Enter your message"
rows="6"></textarea> </div>
            <!-- Break -->
            <div class="12u$">
              <ul class="actions">
                <li><input value="Send Message" type="submit"></li>
                <li><input value="Reset" class="alt" type="reset"></li>
              </ul>
            </div>
          </div>
        </form>
      </div>
    </section>
